
@{
    Layout = null;
}



<!DOCTYPE html>
<html lang="en">

@await Component.InvokeAsync("_HeadComponentPartial")

<body>
    <div class="container-scroller">

        @await Component.InvokeAsync("_SidebarComponentPartial")


        <div class="container-fluid page-body-wrapper">
            @await Component.InvokeAsync("_NavbarComponentPartial")

            <div class="main-panel">

                @RenderBody()

            </div>
        </div>
    </div>
    <script src="~/BreezeAdmin/template/assets/vendors/js/vendor.bundle.base.js"></script>
    <script src="~/BreezeAdmin/template/assets/js/off-canvas.js"></script>
    <script src="~/BreezeAdmin/template/assets/js/hoverable-collapse.js"></script>
    <script src="~/BreezeAdmin/template/assets/js/misc.js"></script>
</body>
</html>
